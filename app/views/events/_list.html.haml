- if events.any?
  .container
    %h3.sub-title
      = local_assigns[:title_label] || @project.try(:component_title, 'event')
      %small.pull-right
        = link_to "이벤트 시작하기", new_event_path(project_id: @project), class: 'btn btn-default btn-inverted btn-sm'

    .row.masonry-container
      = render events
