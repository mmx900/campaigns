(function() {
  <%= render 'campaigns/widget/v1/component' %>
  setTimeout(function() {
    document.querySelectorAll("<%= j params['target_query'] %>").forEach(function (item) {
      if (item.dataset.rendered) { return; }
      zoidInstance().render(item);
      item.dataset.rendered = 'true';
    })
  }, 1000);
})()


