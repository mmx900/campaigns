<% modal_dom_id = "picket-form-modal-#{SecureRandom.uuid}" %>
var partial_picket_form = '<%= j(render "campaigns/picket/picket_form", modal_dom_id: modal_dom_id) %>';
$('body').append(partial_picket_form);
$('#<%= modal_dom_id %>').modal();
$('#<%= modal_dom_id %>').on('hidden.bs.modal',
  function(e) {
    $(this).remove();
  });
parti_partial$($('#<%= modal_dom_id %>'));
