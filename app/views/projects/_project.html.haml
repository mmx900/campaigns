.col-md-4.col-sm-4.col-xs-12
  = link_to project, html_options = {class:'project-link'} do
    .project-card
      .card-image{ style: "height: 200px; background-image: url(#{project.image.md.url});" }
      .project-title
        = project.title
      .project-meta
        %span
          #{project.user.nickname}님의 제안
